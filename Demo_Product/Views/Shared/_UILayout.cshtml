
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

@* Head Alanı Partial_Header *@
<partial name="~/Views/Partials/Partial_Header.cshtml" />

<body>

    @* Navbar için Partial_Navbar *@
    <partial name="~/Views/Partials/Partial_Navbar.cshtml" />

    <div id="layoutSidenav">
        @* Sidebar için Partial_Sidebar *@
        <partial name="~/Views/Partials/Partial_Sidebar.cshtml" />

        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid px-4">
                    <h1 class="mt-4"></h1>
                    @RenderBody()
                </div>
            </main>
            @* Footer İçin Partial_Footer *@
            <partial name="~/Views/Partials/Partial_Footer.cshtml" />
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="~/startbootstrap-sb-admin-gh-pages/js/scripts.js"></script>
</body>
</html>

